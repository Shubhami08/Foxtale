      var dateAf = new Date();
      dateAf.setTime(dateAf.getTime() + 365 * 24 * 60 * 60 * 1000);
      document.cookie = 'affclick=' + v + ';' + 'expires=' + dateAf.toUTCString() + ';path=/';
