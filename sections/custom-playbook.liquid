{% schema %}
  {
    "name": "Playbook",
    "settings": [
      {
        "id": "heading",
        "type": "text",
        "label": "Section Heading"
      }
    ],
    "blocks": [
      {
        "name": "Image",
        "type": "text",
        "settings": [
          {
            "id": "imglink",
            "type": "url",
            "label": "Add Link"
          }, {
            "id": "collectionimg",
            "type": "image_picker",
            "label": "Add Image Desktop"
          }, {
            "id": "collectionimgz",
            "type": "image_picker",
            "label": "Add Image Mobile"
          }, {
            "id": "txtblw",
            "type": "text",
            "label": "Add Card Heading"
          }, {
            "id": "txtblwtht",
            "type": "textarea",
            "label": "Add Content"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Playbook",
        "category": "Any"
      }
    ]
  }
{% endschema %}